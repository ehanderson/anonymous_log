<h1>This is a list of all Posts</h1>
  <table>
    <thead>
      <tr>
        <th>Item ID</th>
        <th>Title</th>
      </tr> 
  </thead>
  
  <tbody>
      <%Post.all.each do |post| %>
        <tr>
          <td><a href="/posts/<%=post.id%>"><%=post.id%></a></td>
          <td><a href="/posts/<%=post.id%>"><%=post.title%></a></td>
      <%end%> 
  </tbody>
</table>
